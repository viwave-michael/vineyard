<table>
  <thead>
    <tr>
      <th>id</th>
      <th>dev_eui</th>
      <th>port</th>
      <th>info</th>
      <th>payload</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @sensor_data.each do |item| %>
    <tr>
      <td><%= item.id %></td>
      <td><%= item.dev_eui %></td>
      <td><%= item.port %></td>
      <td><%= item.info %></td>
      <td><%= item.payload %></td>
      <td><%= link_to "Delete", sensor_datum_path(item), method: :delete %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<%= link_to "New", new_sensor_datum_path %>